  "type": "library",
  "license": "Apache-2.0",
  "homepage": "http://www.geeshoe.com",
  "authors": [
    {
      "name": "Jesse Rushlow",
      "email": "jr@geeshoe.com"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.4",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-phpunit": "^0.12.6"
  },
  "autoload": {
    "psr-4": {
